{
  "kind": "collectionType",
  "collectionName": "equipments",
  "info": {
    "singularName": "equipment",
    "pluralName": "equipments",
    "displayName": "Equipment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string",
      "minLength": 1,
      "maxLength": 150,
      "required": true,
      "unique": true
    },
    "Cover": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "equipment_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::equipment-category.equipment-category",
      "inversedBy": "equipment"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "equipment"
    }
  }
}
